{
  "name": "n8n-nodes-starter",
  "version": "1.0.0",
  "description": "Starter template for n8n community nodes",
  "keywords": [
    "n8n-community-node-package",
    "n8n-nodes",
    "typescript",
    "boilerplate",
    "n8n-agents",
    "workflow-automation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yourusername/n8n-community-node-starter",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/n8n-community-node-starter.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "n8n-node build",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ExampleServiceApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ExampleService/ExampleService.node.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@babel/preset-env": "7.22.10",
    "@n8n/node-cli": "*",
    "@types/jest": "29.5.3",
    "babel-jest": "29.7.0",
    "eslint": "9.32.0",
    "jest": "29.7.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "ts-jest": "29.1.1",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
